{
	"name": "myelectedofficials",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "pnpm run build && wrangler pages dev .svelte-kit/cloudflare",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"deploy": "pnpm run build && wrangler pages deploy .svelte-kit/cloudflare",
		"build-cf-types": "wrangler types && mv worker-configuration.d.ts src/"
	},
	"devDependencies": {
		"@fontsource/fira-mono": "^5.0.13",
		"@neoconfetti/svelte": "^2.2.1",
		"@playwright/test": "^1.46.0",
		"@sveltejs/adapter-auto": "^3.2.3",
		"@sveltejs/adapter-cloudflare": "^4.7.1",
		"@sveltejs/adapter-vercel": "^5.4.2",
		"@sveltejs/kit": "^2.5.21",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@types/eslint": "^9.6.0",
		"@typescript-eslint/eslint-plugin": "^8.0.1",
		"@typescript-eslint/parser": "^8.0.1",
		"autoprefixer": "^10.4.20",
		"eslint": "^9.9.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.43.0",
		"postcss": "^8.4.41",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.6.6",
		"svelte": "^4.2.18",
		"svelte-check": "^3.8.5",
		"tailwindcss": "^3.4.9",
		"tslib": "^2.6.3",
		"typescript": "^5.5.4",
		"vite": "^5.4.0",
		"vitest": "^2.0.5",
		"wrangler": "^3.70.0"
	},
	"type": "module",
	"dependencies": {
		"@google-cloud/vertexai": "^1.4.1",
		"@google/generative-ai": "^0.16.0",
		"@sveltejs/adapter-node": "^5.2.1",
		"bits-ui": "^0.21.13",
		"clsx": "^2.1.1",
		"iconify-icon": "^2.1.0",
		"svelte-markdown": "^0.4.1",
		"svelte-typewriter": "^3.2.3",
		"tailwind-merge": "^2.5.0",
		"tailwind-variants": "^0.2.1",
		"zod": "^3.23.8"
	},
	"packageManager": "pnpm@9.0.6+sha256.0624e30eff866cdeb363b15061bdb7fd9425b17bc1bb42c22f5f4efdea21f6b3"
}
